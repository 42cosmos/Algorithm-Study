   "source": [
    "n = 5\n",
    "lost = [2,4]\n",
    "reserve = [1,3,5]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "dct = {}\n",
    "for i in lost:\n",
    "    dct[i] = i-1, i+1\n",
    "\n",
    "cnt = 0\n",
    "for i, v in enumerate(dct.values()):\n",
    "    for j in reserve:\n",
    "        if j in v:\n",
    "            cnt += 1\n",
    "            break\n",
    "            \n",
    "return n - (len(lost) - cnt)"
   ]
  },
